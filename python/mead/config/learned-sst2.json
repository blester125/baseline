{
    "task": "classify",
    "batchsz": 50,
    "preproc": {
	"mxlen": 100,
	"rev": false,
	"clean": true
    },
    "backend": "tensorflow",
    "dataset": "SST2",
    "loader": {
	"reader_type": "default"
    },
    "unif": 0.25,
    "model": {
	"model_type": "learn_init",
    "rnnsz": 200,
	"dropout": 0.5,
    "rnntype": "lstm",
    "learn_init": true,
	"finetune": true
    },
    "word_embeddings": {
	"label": "w2v-gn"
    },
    "train": {
	"epochs": 3,
	"optim": "adadelta",
	"eta": 1.0,
	"model_base": "./models/sst2",
	"early_stopping_metric": "acc",
        "verbose": true
    }
}
